[core]
	editor = vi

[alias]
	graph = log --graph
	cb = !git branch | grep -v -e "$(git branch --show-current)" -e "main" -e "master" | xargs git branch -d

[credential "https://github.com"]
	helper =
	helper = !gh auth git-credential

[credential "https://gist.github.com"]
	helper =
	helper = !gh auth git-credential
