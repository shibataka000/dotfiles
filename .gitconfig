[core]
	editor = vi

[alias]
	graph = log --graph
	cb = !git branch | grep -v -e "$(git branch --show-current)" -e "$(git remote show origin | grep 'HEAD branch' | awk '{print $NF}')" | xargs git branch -d

[credential "https://github.com"]
	helper =
	helper = !gh auth git-credential

[credential "https://gist.github.com"]
	helper =
	helper = !gh auth git-credential
